{
    "name": "theodo/roger-cms-bundle",
    "type": "symfony-bundle",
    "description": "Symfony port of radiant CMS",
    "keywords": ["cms"],
    "license": "MIT",
    "authors": [
        {
            "name": "Theodo",
            "email": "contact@theodo.fr"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": ">=2.2.0, <2.4",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": ">=1.0.0, <1.3",
        "stof/doctrine-extensions-bundle": ">=1.0.0, < 1.2",
        "doctrine/doctrine-fixtures-bundle": "dev-master"
    },
    "require-dev": {
        "phake/phake": "dev-master",
        "doctrine/data-fixtures": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Theodo\\RogerCmsBundle": ""
        }
    },
    "target-dir": "Theodo/RogerCmsBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
